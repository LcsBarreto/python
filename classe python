class Pessoa:
    def __init__(self, nome, ender):
        self.nome = nome
        self.ender = ender

    def set_nome(self, nome):
        self.nome = nome

    def set_ender(self, ender):
        self.ender = ender

    def get_nome(self):
        return self.nome

    def get_ender(self):
        return self.ender

pessoa1 = Pessoa("lucas", "rua ildefonso albano")
pessoa2 = Pessoa("joana", "rua rodrigues junior")

print(f'Nome: {pessoa1.get_nome()}, endereÃ§o: {pessoa1.get_ender()}')
print(f'Nome: {pessoa2.get_nome()}, endereÃ§o: {pessoa2.get_ender()}')
